/* pages/history/history.wxss */
.plan{
  width:650rpx;
  height: auto;
  margin-left:50rpx;
  margin-top:30rpx;
  background-color: #F26157;
  color:#ffffff;
  border-radius: 20rpx;
  padding-bottom: 20rpx;
}
.planName{
  float:left;
  border-bottom:2rpx solid #ffffff;
  width: 325rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  border-right: 1rpx solid #ffffff;
}
.Time{
  float:left;
  border-bottom:2rpx solid #ffffff;
  width: 324rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
}
.planGoal{
  line-height: 60rpx;
  text-align: center;
  width:325rpx;
  height:60rpx;
  float:left;
  border-right: 1rpx solid #ffffff;
  border-bottom:2rpx solid #ffffff;
}
.planTime{
  line-height: 60rpx;
  text-align: center;
  float:left;
  width:324rpx;
  height:60rpx;
  border-bottom:2rpx solid #ffffff;
}
.planBut{
  clear:both;
  height:110rpx;
  color:#ffffff;
  background-color: #F1A66A;
  border-top-left-radius: 0px;
  border-top-right-radius: 0px;
}
.planDesc{
  padding-top:10rpx;
  padding-left:20rpx;
  width:100%;
  clear: both;
  border-bottom:2rpx solid #ffffff;
}
.starimgBox{
  margin-top:10rpx;
  margin-left:125rpx;
}
image{
  margin-top:20rpx;
  float:left;
  width:80rpx;
  height:80rpx;
}
.summary{
  clear:both;
  padding-top:10rpx;
  padding-left:20rpx;
}